    {% comment %}
      {% for link in section.settings.menu.links %}
        {{ link.title | link_to: link.url }}
      {% endfor %}
    {% endcomment %}

    {%- liquid
      if section.settings.menu != blank
        render 'header-dropdown-menu'
      else
        echo 'No menu links found.'
      endif
    %}
  </div>

  <div class="header-icons">
    {% if shop.customer_accounts_enabled %}
      {{ 'icon-account.svg' | inline_asset_content | link_to: routes.account_url }}
    {% endif %}

    <a href="{{ routes.cart_url }}">
      {% if cart.item_count > 0 %}
        <sup>{{ cart.item_count }}</sup>
      {% endif %}

      {{ 'icon-cart.svg' | inline_asset_content }}
    </a>
  </div>
</header>

{% stylesheet %}
  header {
    height: 5rem;
    display: flex;
    align-items: center;
    justify-content: space-between;
  }

  header a {
    position: relative;
    text-decoration: none;
    color: var(--color-dark);
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
    font-size: var(--font-size-base);
    font-weight: 500;
  }

  header a sup {
    position: absolute;
    right: 2px;
    bottom: 0;
    overflow: hidden;
    max-width: var(--page-margin);
    background: var(--color-dark);
    color: var(--color-light);
    width: 18px;
    height: 18px;
    min-width: 18px;
    min-height: 18px;
    display: flex;
    align-items: center;
    justify-content: center;
    border-radius: var(--br-circle);
    font-size: var(--font-size-xs);
  }

  .header-icons svg {
    width: 2rem;
  }

  header .logo {
    display: flex;
    align-items: center;
    height: 100%;
    width: auto;
    min-width: 100px;
    margin-right: var(--spacing-md);
  }

  header .header-menu,
  header .header-icons {
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
    gap: 1rem;
  }
{% endstylesheet %}

{% schema %}
{
  "name": "t:general.header",
  "settings": [
    {
      "type": "link_list",
      "id": "menu",
      "label": "t:labels.menu"
    }
  ]
}
{% endschema %}
